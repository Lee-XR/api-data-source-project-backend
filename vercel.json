{
    "functions": {
		"api/skiddle-api-php/*.php": {
			"runtime": "vercel-php@0.5.3"
		},
		"api/data-process-node/*.js": {
			"runtime": "@vercel/node@2.15.1"
		}
	},
	"rewrites": [
		{
			"source": "/api/skiddle/(.*)",
			"destination": "/api/skiddle-api-php/index.php"
		},
		{
			"source": "/data-process/(.*)",
			"destination": "/api/data-process-node/index.js"
		}
	],
	"headers": [
		{
			"source": "/api/(.*)",
			"headers": [
				{ "key": "Access-Control-Allow-Origin", "value": "https://api-data-source-project-git-node-preprocess-lee-xr.vercel.app" },
				{ "key": "Access-Control-Allow-Methods", "value": "OPTIONS, GET, POST" },
				{ "key": "Access-Control-Allow-Headers", "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version" }
			]
		}
	]
}
